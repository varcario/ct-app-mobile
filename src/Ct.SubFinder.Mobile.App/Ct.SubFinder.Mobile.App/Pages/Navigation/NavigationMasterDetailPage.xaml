<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
               xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
               x:Class="Ct.SubFinder.Mobile.App.Pages.Navigation.NavigationMasterDetailPage"
               xmlns:pages="clr-namespace:Ct.SubFinder.Mobile.App.Pages.Navigation"               
               IsPresented="{Binding IsFlyoutOpen, Mode=TwoWay}">
    <MasterDetailPage.Master>
        <NavigationPage Title="Menu" Icon="ic_home_black_24dp.png">
            <x:Arguments>
                <pages:NavigationMasterDetailPageMaster/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Master>
</MasterDetailPage>